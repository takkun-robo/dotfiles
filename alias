
# aliases
alias ls='ls -F --color=auto'
alias ll='ls -alF'
alias la='ls -AF'
alias l='ls -CF'

alias gr='grep'

alias ..='cd ../'
alias ..2='cd ../../'

alias t='touch'

alias cp='cp -i'
alias mv='mv -i'
#alias rm='rm -i'

alias et='exit'

alias v='vim'
alias vd='vimdiff'

alias tm='tmux'
alias tml='tmux ls'
alias tma='tmux attach'

alias g='git'
alias ga='git add'
alias gs='git status'
alias gd='git diff'
alias gc='git commit'
alias gcm='git commit -m'
alias gf='git fetch'
alias gm='git merge'
alias gpsh='git push'
alias gpll='git pull'

